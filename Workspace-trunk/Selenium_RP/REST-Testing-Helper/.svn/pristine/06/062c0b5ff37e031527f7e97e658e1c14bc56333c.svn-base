package uk.co.ratedpeople;


public class DetailedAddressBuilder {

	public static  DetailedAddress build(String property, String street, String town, String county, String postcode) {
		 DetailedAddress obj = new  DetailedAddress();
		obj.setCounty(county);
		obj.setPostcode(postcode);
		obj.setProperty(property);
		obj.setStreet(street);
		obj.setTown(town);
		return obj;
	}
}

